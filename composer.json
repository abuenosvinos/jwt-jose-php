{
    "name": "antonio/jwt-jose-php",
    "description": "Implementation of JWT using https://github.com/web-token libraries",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Abuenosvinos\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abuenosvinos\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Antonio Buenosvinos",
            "email": "antonio.buenosvinos@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "web-token/jwt-core": "^3.2",
        "web-token/jwt-encryption": "^3.2",
        "web-token/jwt-encryption-algorithm-aesgcm": "^3.2",
        "web-token/jwt-encryption-algorithm-dir": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    }
}
